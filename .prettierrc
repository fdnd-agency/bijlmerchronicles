{
    "plugins": ["prettier-plugin-svelte"],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ],
    "svelteSortOrder": "options-scripts-markup-styles",
    "semi": true,
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 4
}
